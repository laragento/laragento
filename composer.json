{
    "name": "laragento/laragento",
    "description": "The Laragento Framework.",
    "keywords": ["framework", "laravel","magento","laragento"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "~4.0",
        "laragento/catalog": "dev-master",
        "laragento/catalog-import-export": "dev-master",
        "laragento/catalog-url-rewrite": "dev-master",
        "laragento/checkout": "dev-master",
        "laragento/core": "dev-master",
        "laragento/customer": "dev-master",
        "laragento/customer-import-export": "dev-master",
        "laragento/directory": "dev-master",
        "laragento/eav": "dev-master",
        "laragento/import-export": "dev-master",
        "laragento/media-storage": "dev-master",
        "laragento/quote": "dev-master",
        "laragento/rating": "dev-master",
        "laragento/review": "dev-master",
        "laragento/store": "dev-master",
        "laragento/xml-chunk": "dev-master",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "mpociot/laravel-apidoc-generator": "^2.0",
        "predis/predis": "^1.1",
        "prewk/xml-string-streamer": "^0.11.0",
        "spatie/laravel-fractal": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "~3.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/laragento/catalog"
        },
        {
            "type": "path",
            "url": "./packages/laragento/catalog-import-export"
        },
        {
            "type": "path",
            "url": "./packages/laragento/catalog-url-rewrite"
        },       
        {
            "type": "path",
            "url": "./packages/laragento/checkout"
        },
        {
            "type": "path",
            "url": "./packages/laragento/core"
        },
        {
            "type": "path",
            "url": "./packages/laragento/customer"
        },
        {
            "type": "path",
            "url": "./packages/laragento/customer-import-export"
        },
        {
            "type": "path",
            "url": "./packages/laragento/directory"
        },
        {
            "type": "path",
            "url": "./packages/laragento/eav"
        },
        {
            "type": "path",
            "url": "./packages/laragento/import-export"
        },
        {
            "type": "path",
            "url": "./packages/laragento/media-storage"
        },
        {
            "type": "path",
            "url": "./packages/laragento/quote"
        },
        {
            "type": "path",
            "url": "./packages/laragento/rating"
        },
        {
            "type": "path",
            "url": "./packages/laragento/review"
        },
        {
            "type": "path",
            "url": "./packages/laragento/store"
        },
        {
            "type": "path",
            "url": "./packages/laragento/xml-chunk"
        }
      ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
            
            
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
