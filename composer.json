{
    "name": "laragento/laragento",
    "description": "The Laragento Framework.",
    "keywords": [
        "framework",
        "laravel",
        "magento",
        "laragento"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "./Catalog",
            "options": {
              "symlink": true
            }
        },
        {
            "type": "path",
