            "url": "./CatalogUrlRewrite",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Checkout",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Core",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Customer",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./CustomerImportExport",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Directory",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Eav",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./ImportExport",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
