{
    "name": "laragento/laragento",
    "description": "The Laragento Framework.",
    "keywords": [
        "framework",
        "laravel",
        "magento",
        "laragento"
    ],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-ide-helper": "^2.3@dev",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "mpociot/laravel-apidoc-generator": "^2.0",
        "nwidart/laravel-modules": "^3.0@dev",
        "predis/predis": "^1.1",
        "prewk/xml-string-streamer": "^0.11.0",
        "spatie/laravel-fractal": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "facebook/webdriver": "^1.5",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "~3.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Laragento\\": "Laragento/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "Laragento/Store"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}