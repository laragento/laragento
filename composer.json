          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Quote",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Rating",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Review",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./Store",
          "options": {
            "symlink": true
          }
        },
        {
            "type": "path",
            "url": "./XmlChunk",
          "options": {
            "symlink": true
          }
        }
